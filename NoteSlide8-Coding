package myWhile;

public class NoteSlide8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;
		int j=10;
		int k=20;
		
		for (i = 1; i < 5; i++ ) {
			System.out.print(i + " ");
		}
		
		System.out.println();
		
		for (j = 10; j <20; j++) {
			System.out.print(j + " ");
		}
		
		System.out.println();
		
		for (k = 20; k > 10; k--) {
			System.out.print(k + " ");
		}
	}

}
