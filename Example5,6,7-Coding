package selfProgramming;

public class Example5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//EXAMPLE 5
		for (int i=1; i<5; i++)
			System.out.print(i + " ");
		
		System.out.println();
		
		//EXAMPLE 6
		for (int i=1; i<3; i++) {
			System.out.print("YAHOO"+ " ");
			System.out.print("* * * ");
		}
		
		System.out.println();
		
		//EXAMPLE 7
		for (int i=1; i<=5; i++) {
			System.out.print("YAHOO ");
		}
		System.out.print(" *** ");
			
	}

}
